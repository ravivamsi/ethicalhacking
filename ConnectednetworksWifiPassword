# Show the networks with the SSID
netsh wlan show networks mode=bssid

# Connect to the specific Wifiname
netsh wlan connect name=Wifiname

# Show the profiles already connected
netsh wlan show profiles

# Show the details of the profile
netsh wlan show profiles Wifiname key=content

# Show the password of the profile 
netsh wlan show profiles Wifiname key=clear

# Disconnect the WLAN connected
netsh wlan disconnect
